{
  "name": "nouns-tab",
  "description": "Nouns Tab",
  "version": "0.1",
  "manifest_version": 3,
  "icons": {
    "16": "static/noggles16.png",
    "32": "static/noggles32.png",
    "48": "static/noggles48.png",
    "128": "static/noggles128.png"
  },
  "background": {
    "service_worker": "src/background/index.ts"
  },
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": false
  },
  "chrome_url_overrides": {
    "newtab": "src/nouns-tab/nouns-tab.html"
  },
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "static/noggles16.png",
      "32": "static/noggles32.png",
      "48": "static/noggles48.png",
      "128": "static/noggles128.png"
    }
  },
  "permissions": ["storage"]
}
